{
    "name": "bambamboole/laravel-kickstart",
    "type": "project",
    "description": "An opinionated starter kit for Laravel projects.",
    "keywords": [
        "laravel",
        "framework",
        "starterkit",
        "inertia",
        "vue"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ajcastro/eager-load-pivot-relations": "^0.3.0",
        "bambamboole/laravel-i18next": "^0.3.0",
        "inertiajs/inertia-laravel": "^1.3",
        "laravel/framework": "^11.16",
        "laravel/reverb": "^1.0.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "spatie/laravel-query-builder": "^6.0",
        "tightenco/ziggy": "^2.21"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.13",
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.16",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.3",
        "pestphp/pest": "^2.23",
        "pestphp/pest-plugin-laravel": "^2.4",
        "pestphp/pest-plugin-type-coverage": "^2.8",
        "zircote/swagger-php": "^4.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "lint": "@php ./vendor/bin/pint",
        "test": "@php artisan test --parallel"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
